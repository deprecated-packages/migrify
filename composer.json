{
    "name": "migrify/migrify",
    "description": "It's Migration Time, Baby!",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-dom": "*",
        "ext-pdo": "*",
        "ext-simplexml": "*",
        "cweagans/composer-patches": "^1.6",
        "friendsofphp/php-cs-fixer": "^2.16",
        "knplabs/doctrine-behaviors": "^2.0.7",
        "nette/neon": "^3.0",
        "nette/robot-loader": "^3.2",
        "nette/utils": "^3.1",
        "nikic/php-parser": "^4.9",
        "psr/container": "^1.0",
        "sebastian/diff": "^3.0|^4.0",
        "squizlabs/php_codesniffer": "^3.5.6",
        "symfony/console": "^4.4|^5.1",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/expression-language": "^4.4|^5.1",
        "symfony/finder": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/yaml": "^4.4|^5.1",
        "symplify/autowire-array-parameter": "dev-master",
        "symplify/composer-json-manipulator": "dev-master",
        "symplify/console-color-diff": "dev-master",
        "symplify/package-builder": "dev-master",
        "symplify/php-config-printer": "dev-master",
        "symplify/smart-file-system": "dev-master",
        "symplify/symplify-kernel": "dev-master"
    },
    "require-dev": {
        "composer/composer": "^1.10",
        "dama/doctrine-test-bundle": "^6.3",
        "doctrine/annotations": "^1.10",
        "doctrine/cache": "^1.10",
        "doctrine/dbal": "^2.10",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "latte/latte": "^2.8",
        "nette/application": "^3.0.4",
        "phpstan/phpstan": "^0.12.38",
        "phpstan/phpstan-phpunit": "^0.12.16",
        "phpstan/phpstan-symfony": "^0.12.6",
        "phpunit/phpunit": "^8.5|^9.0",
        "slevomat/coding-standard": "^6.3.11",
        "symfony/framework-bundle": "^4.4|^5.1",
        "symfony/routing": "^4.4|^5.1",
        "symfony/security-bundle": "^4.4|^5.1",
        "symfony/security-core": "^4.4|^5.1",
        "symplify/changelog-linker": "dev-master",
        "symplify/easy-coding-standard": "dev-master",
        "symplify/easy-testing": "dev-master",
        "symplify/monorepo-builder": "dev-master",
        "symplify/phpstan-extensions": "dev-master",
        "symplify/phpstan-rules": "dev-master",
        "tracy/tracy": "^2.7"
    },
    "autoload": {
        "files": [
            "packages/sniffer-fixer-to-ecs/stubs/Sniff.php"
        ],
        "psr-4": {
            "Migrify\\ClassPresence\\": "packages/class-presence/src",
            "Migrify\\ConfigFeatureBumper\\": "packages/config-feature-bumper/src",
            "Migrify\\ConfigPretifier\\": "packages/config-pretifier/src",
            "Migrify\\ConfigTransformer\\": "packages/config-transformer/src",
            "Migrify\\ConfigTransformer\\Clarifier\\": "packages/config-transformer/packages/clarifier/src",
            "Migrify\\ConfigTransformer\\FeatureShifter\\": "packages/config-transformer/packages/feature-shifter/src",
            "Migrify\\ConfigTransformer\\FormatSwitcher\\": "packages/config-transformer/packages/format-switcher/src",
            "Migrify\\DiffDataMiner\\": "packages/diff-data-miner/src",
            "Migrify\\EasyCI\\": "packages/easy-ci/src",
            "Migrify\\LatteToTwig\\": "packages/latte-to-twig/src",
            "Migrify\\MigrifyKernel\\": "packages/migrify-kernel/src",
            "Migrify\\NeonToYaml\\": "packages/neon-to-yaml/src",
            "Migrify\\PHPMDDecomposer\\": "packages/phpmd-decomposer/src",
            "Migrify\\PHPUnitUpgrader\\": "packages/phpunit-upgrader/src",
            "Migrify\\PhpConfigPrinter\\": "packages/php-config-printer/src",
            "Migrify\\Psr4Switcher\\": "packages/psr4-switcher/src",
            "Migrify\\SnifferFixerToECS\\": "packages/sniffer-fixer-to-ecs/src",
            "Migrify\\StaticDetector\\": "packages/static-detector/src",
            "Migrify\\SymfonyRouteUsage\\": "packages/symfony-route-usage/src",
            "Migrify\\TemplateChecker\\": "packages/template-checker/src",
            "Migrify\\TravisToGithubActions\\": "packages/travis-to-github-actions/src",
            "Migrify\\VendorPatches\\": "packages/vendor-patches/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\ClassPresence\\Tests\\": "packages/class-presence/tests",
            "Migrify\\ConfigFeatureBumper\\Tests\\": "packages/config-feature-bumper/tests",
            "Migrify\\ConfigPretifier\\Tests\\": "packages/config-pretifier/tests",
            "Migrify\\ConfigTransformer\\Clarifier\\Tests\\": "packages/config-transformer/packages/clarifier/tests",
            "Migrify\\ConfigTransformer\\FeatureShifter\\Tests\\": "packages/config-transformer/packages/feature-shifter/tests",
            "Migrify\\ConfigTransformer\\FormatSwitcher\\Tests\\": "packages/config-transformer/packages/format-switcher/tests",
            "Migrify\\ConfigTransformer\\Tests\\": "packages/config-transformer/tests",
            "Migrify\\DiffDataMiner\\Tests\\": "packages/diff-data-miner/tests",
            "Migrify\\EasyCI\\Tests\\": "packages/easy-ci/tests",
            "Migrify\\LatteToTwig\\Tests\\": "packages/latte-to-twig/tests",
            "Migrify\\MigrifyKernel\\Tests\\": "packages/migrify-kernel/tests",
            "Migrify\\NeonToYaml\\Tests\\": "packages/neon-to-yaml/tests",
            "Migrify\\PHPMDDecomposer\\Tests\\": "packages/phpmd-decomposer/tests",
            "Migrify\\PHPUnitUpgrader\\Tests\\": "packages/phpunit-upgrader/tests",
            "Migrify\\PhpConfigPrinter\\Tests\\": "packages/php-config-printer/tests",
            "Migrify\\Psr4Switcher\\Tests\\": "packages/psr4-switcher/tests",
            "Migrify\\SnifferFixerToECS\\Tests\\": "packages/sniffer-fixer-to-ecs/tests",
            "Migrify\\StaticDetector\\Tests\\": "packages/static-detector/tests",
            "Migrify\\SymfonyRouteUsage\\Tests\\": "packages/symfony-route-usage/tests",
            "Migrify\\TemplateChecker\\Tests\\": "packages/template-checker/tests",
            "Migrify\\TravisToGithubActions\\Tests\\": "packages/travis-to-github-actions/tests",
            "Migrify\\VendorPatches\\Tests\\": "packages/vendor-patches/tests"
        },
        "files": [
            "vendor/symfony/dependency-injection/Loader/Configurator/ContainerConfigurator.php"
        ]
    },
    "replace": {
        "migrify/class-presence": "0.4",
        "migrify/config-feature-bumper": "0.4",
        "migrify/config-pretifier": "0.4",
        "migrify/config-transformer": "0.4",
        "migrify/diff-data-miner": "0.4",
        "migrify/easy-ci": "0.4",
        "migrify/latte-to-twig": "0.4",
        "migrify/migrify-kernel": "0.4",
        "migrify/neon-to-yaml": "0.4",
        "migrify/php-config-printer": "0.4",
        "migrify/phpmd-decomposer": "0.4",
        "migrify/phpunit-upgrader": "0.4",
        "migrify/psr4-switcher": "0.4",
        "migrify/sniffer-fixer-to-ecs": "0.4",
        "migrify/static-detector": "0.4",
        "migrify/symfony-route-usage": "0.4",
        "migrify/template-checker": "0.4",
        "migrify/travis-to-github-actions": "0.4",
        "migrify/vendor-patches": "0.4"
    },
    "scripts": {
        "check-cs": "vendor/bin/ecs check packages --ansi",
        "fix-cs": "vendor/bin/ecs check packages --fix --ansi",
        "phpstan": "vendor/bin/phpstan analyse packages --error-format symplify --ansi",
        "merge": "vendor/bin/monorepo-builder merge --ansi",
        "propagate": "vendor/bin/monorepo-builder propagate --ansi",
        "changelog": [
            "vendor/bin/changelog-linker dump-merges --in-categories --in-packages --ansi",
            "vendor/bin/changelog-linker link --ansi",
            "vendor/bin/changelog-linker cleanup --ansi"
        ],
        "validate-monorepo": "vendor/bin/monorepo-builder validate --ansi",
        "rector": "vendor/bin/rector process packages --config rector-ci.php --ansi",
        "rector-ci": "vendor/bin/rector process packages --config rector-ci.php --dry-run --ansi",
        "sonar": "packages/easy-ci/bin/easy-ci generate-sonar --ansi",
        "release": "vendor/bin/monorepo-builder release patch --ansi"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
