{
    "name": "migrify/easy-ci",
    "description": "Tools that you use in any CI",
    "license": "MIT",
    "bin": [
        "bin/easy-ci"
    ],
    "require": {
        "php": ">=7.2",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/yaml": "^4.4|^5.1",
        "nette/utils": "^3.1",
        "symplify/package-builder": "^8.2",
        "symplify/smart-file-system": "^8.2",
        "migrify/migrify-kernel": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\EasyCI\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\EasyCI\\Tests\\": "tests"
        }
    }
}
