{
    "name": "migrify/class-presence",
    "description": "Check NEON/YAML/TWIG/LATTE files for existing classes",
    "license": "MIT",
    "bin": [
        "bin/class-presence"
    ],
    "require": {
        "php": ">=7.2",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/yaml": "^4.4|^5.1",
        "nette/utils": "^3.1",
        "nette/neon": "^3.0",
        "symplify/package-builder": "^8.2",
        "migrify/migrify-kernel": "^0.4",
        "symplify/smart-file-system": "^8.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\ClassPresence\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\ClassPresence\\Tests\\": "tests"
        }
    }
}
