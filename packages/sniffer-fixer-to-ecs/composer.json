{
    "name": "migrify/sniffer-fixer-to-ecs",
    "description": "Switch PHP_CodeSniffer or PHP-CS-Fixer to ECS with Single Command",
    "license": "MIT",
    "bin": [
        "bin/sniffer-fixer-to-ecs"
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-simplexml": "*",
        "nette/utils": "^3.1",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/package-builder": "^8.1.17",
        "symplify/autowire-array-parameter": "^8.1.17",
        "symplify/smart-file-system": "^8.1.17",
        "migrify/php-config-printer": "^0.3.33"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\SnifferFixerToECS\\": "src"
        },
        "classmap": [
            "vendor/squizlabs/php_codesniffer/src"
        ],
        "files": [
            "stubs/Sniff.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\SnifferFixerToECS\\Tests\\": "tests"
        }
    }
}
