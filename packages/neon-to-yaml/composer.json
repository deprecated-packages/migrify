{
    "name": "migrify/neon-to-yaml",
    "description": "Converts Neon to Yaml",
    "license": "MIT",
    "bin": [
        "bin/neon-to-yaml"
    ],
    "require": {
        "php": ">=7.2",
        "nette/utils": "^3.1",
        "nette/neon": "^3.0",
        "symplify/package-builder": "^8.2",
        "symplify/smart-file-system": "^8.2",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/yaml": "^4.4|^5.1",
        "migrify/migrify-kernel": "^0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\NeonToYaml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\NeonToYaml\\Tests\\": "tests"
        }
    }
}
