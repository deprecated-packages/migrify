{
    "name": "migrify/neon-to-yaml",
    "description": "Converts Neon to Yaml",
    "license": "MIT",
    "bin": [
        "bin/neon-to-yaml"
    ],
    "require": {
        "php": "^7.2|^8.0",
        "nette/utils": "^3.1",
        "nette/neon": "^3.0",
        "symplify/package-builder": "^8.1.17",
        "symplify/smart-file-system": "^8.1.17",
        "symplify/autowire-array-parameter": "^8.1.17",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/yaml": "^4.4|^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\NeonToYaml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\NeonToYaml\\Tests\\": "tests"
        }
    }
}
