{
    "name": "symplify/neon-to-yaml",
    "description": "Converts Neon to Yaml",
    "license": "MIT",
    "bin": [
        "bin/neon-to-yaml"
    ],
    "require": {
        "php": "^7.2",
        "nette/utils": "^3.1",
        "nette/neon": "^3.0",
        "symplify/package-builder": "^7.2",
        "symplify/smart-file-system": "^7.2",
        "symplify/autowire-array-parameter": "^7.2",
        "symfony/http-kernel": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\NeonToYaml\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\NeonToYaml\\Tests\\": "tests"
        }
    }
}
