{
    "name": "migrify/template-checker",
    "description": "Check your TWIG templates",
    "license": "MIT",
    "bin": [
        "bin/template-checker"
    ],
    "require": {
        "php": ">=7.2",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/yaml": "^4.4|^5.1",
        "nette/utils": "^3.1",
        "nikic/php-parser": "^4.9",
        "migrify/migrify-kernel": "^0.3.55"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "nette/application": "^3.0.4",
        "latte/latte": "^2.8"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\TemplateChecker\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\TemplateChecker\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
