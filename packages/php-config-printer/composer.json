{
    "name": "migrify/php-config-printer",
    "description": "Print Symfony services array with configuration to to plain PHP file format thanks to this simple php-parser wrapper",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "nette/utils": "^3.1",
        "nikic/php-parser": "^4.9",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/package-builder": "^8.2",
        "symplify/smart-file-system": "^8.2",
        "migrify/migrify-kernel": "^0.4"
    },
    "require-dev": {
        "symplify/easy-testing": "^8.2",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\PhpConfigPrinter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\PhpConfigPrinter\\Tests\\": "tests"
        }
    }
}
