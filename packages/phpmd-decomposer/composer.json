{
    "name": "migrify/phpmd-decomposer",
    "description": "Decompose phpmd.xml to PHPStan, ECS and Rector rules",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "nette/utils": "^3.1",
        "nette/neon": "^3.0",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/package-builder": "^8.3.35",
        "migrify/php-config-printer": "^0.3.50",
        "migrify/migrify-kernel": "^0.3.50"
    },
    "require-dev": {
        "symplify/easy-testing": "^8.3.35",
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\PHPMDDecomposer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\PHPMDDecomposer\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
