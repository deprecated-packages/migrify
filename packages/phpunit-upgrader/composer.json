{
    "name": "migrify/phpunit-upgrader",
    "description": "Upgrade PHPUnit tests with smart helping hand",
    "license": "MIT",
    "bin": [
        "bin/phpunit-upgrader"
    ],
    "require": {
        "php": ">=7.2",
        "nette/utils": "^3.1",
        "nette/robot-loader": "^3.2",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/console-color-diff": "dev-master",
        "symplify/package-builder": "dev-master",
        "symplify/smart-file-system": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\PHPUnitUpgrader\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\PHPUnitUpgrader\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
