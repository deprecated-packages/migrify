{
    "name": "migrify/symfony-route-usage",
    "description": "Detect used and unused Symfony routes",
    "type": "symfony-bundle",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "ext-pdo": "*",
        "symplify/package-builder": "^8.3.35",
        "symplify/smart-file-system": "^8.3.35",
        "knplabs/doctrine-behaviors": "^2.0.7"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.3",
        "doctrine/annotations": "^1.10",
        "doctrine/cache": "^1.10",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/dbal": "^2.10",
        "doctrine/orm": "^2.7",
        "phpunit/phpunit": "^8.5|^9.0",
        "symfony/routing": "^4.4|^5.1",
        "symfony/security-core": "^4.4|^5.1",
        "symfony/security-bundle": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symfony/framework-bundle": "^4.4|^5.1"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\SymfonyRouteUsage\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\SymfonyRouteUsage\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
