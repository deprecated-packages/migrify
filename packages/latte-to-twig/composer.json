{
    "name": "migrify/latte-to-twig",
    "description": "Latte to Twig converter converts Latte to Twig",
    "license": "MIT",
    "bin": [
        "bin/latte-to-twig"
    ],
    "require": {
        "php": "^7.2",
        "nette/utils": "^3.1",
        "symfony/dependency-injection": "^5.0",
        "symfony/console": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/finder": "^5.0",
        "symplify/package-builder": "^7.2",
        "symplify/autowire-array-parameter": "^7.2",
        "symplify/smart-file-system": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\LatteToTwig\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\LatteToTwig\\Tests\\": "tests"
        }
    }
}
