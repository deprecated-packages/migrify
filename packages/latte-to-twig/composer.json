{
    "name": "migrify/latte-to-twig",
    "description": "Latte to Twig converter converts Latte to Twig",
    "license": "MIT",
    "bin": "bin/latte-to-twig",
    "require": {
        "php": "^7.2",
        "nette/utils": "^3.1",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/finder": "^4.4|^5.0",
        "symplify/package-builder": "^8.0",
        "symplify/autowire-array-parameter": "^8.0",
        "symplify/smart-file-system": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\LatteToTwig\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\LatteToTwig\\Tests\\": "tests"
        }
    }
}
