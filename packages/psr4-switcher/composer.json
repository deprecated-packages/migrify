{
    "name": "migrify/psr4-switcher",
    "description": "Switch classmap to PSR-4",
    "license": "MIT",
    "bin": [
        "bin/psr4-switcher"
    ],
    "require": {
        "php": ">=7.2",
        "nette/utils": "^3.1",
        "nette/robot-loader": "^3.2",
        "migrify/migrify-kernel": "^0.5",
        "symfony/dependency-injection": "^4.4|^5.1",
        "symfony/console": "^4.4|^5.1",
        "symfony/http-kernel": "^4.4|^5.1",
        "symplify/composer-json-manipulator": "dev-master",
        "symplify/package-builder": "dev-master",
        "symplify/smart-file-system": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\Psr4Switcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\Psr4Switcher\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
