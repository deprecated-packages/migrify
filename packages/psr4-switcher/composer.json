{
    "name": "migrify/psr4-switcher",
    "description": "Switch classmap to PSR-4",
    "license": "MIT",
    "bin": [
        "bin/psr4-switcher"
    ],
    "require": {
        "php": "^7.2",
        "nette/utils": "^3.1",
        "nette/robot-loader": "^3.2",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symplify/composer-json-manipulator": "^7.3.10",
        "symplify/autowire-array-parameter": "^7.3.10",
        "symplify/package-builder": "^7.3.10",
        "symplify/smart-file-system": "^7.3.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\Psr4Switcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\Psr4Switcher\\Tests\\": "tests"
        }
    }
}
