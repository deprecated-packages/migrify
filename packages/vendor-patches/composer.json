{
    "name": "migrify/vendor-patches",
    "description": "Generate vendor patches for packages with single command",
    "type": "composer-plugin",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "composer-plugin-api": "^1.1",
        "nette/utils": "^3.1",
        "symplify/smart-file-system": "^8.1",
        "symplify/package-builder": "^8.1",
        "symplify/autowire-array-parameter": "^8.1",
        "cweagans/composer-patches": "^1.6",
        "sebastian/diff": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\VendorPatches\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\VendorPatches\\Tests\\": "tests"
        }
    },
    "extra": {
        "class": "Migrify\\VendorPatches\\Composer\\Plugin"
    }
}
