{
    "name": "migrify/zephir-to-php",
    "description": "Convert Zephir (Phalcon custom language) to normal PHP. Useful for simple autoloading of static tools like Rector, PHPStan and ECS",
    "license": "MIT",
    "bin": [
        "bin/zephir-to-php"
    ],
    "require": {
        "php": "^7.2|^8.0",
        "nette/utils": "^3.1",
        "symfony/dependency-injection": "^4.4|^5.0",
        "symfony/console": "^4.4|^5.0",
        "symfony/http-kernel": "^4.4|^5.0",
        "symfony/finder": "^4.4|^5.0",
        "symplify/package-builder": "^8.1.17",
        "symplify/autowire-array-parameter": "^8.1.17",
        "symplify/smart-file-system": "^8.1.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0",
        "phalcon/cphalcon": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Migrify\\ZephirToPhp\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Migrify\\ZephirToPhp\\Tests\\": "tests"
        }
    }
}
